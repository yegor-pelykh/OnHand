{
    "manifest_version": 2,
    "name": "OnHand",
    "short_name": "OnHand",
    "version": "1.0.0",
    "description": "An open source browser extension to save your favorite bookmarks on a \"new tab\" page.",
    "author": "Yegor Pelykh",
    "developer": {
        "name": "Yegor Pelykh",
        "url": "https://github.com/yegor-pelykh"
    },
    "homepage_url": "https://github.com/yegor-pelykh/OnHand",
    "incognito": "spanning",
    "icons": {
        "16": "icons/icon-16.png",
        "24": "icons/icon-24.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "applications": {
        "gecko": {
            "id": "{486606f1-39f0-4662-b43f-1e629b18f89e}"
        }
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "<all_urls>",
        "tabs",
        "unlimitedStorage"
    ]
}
